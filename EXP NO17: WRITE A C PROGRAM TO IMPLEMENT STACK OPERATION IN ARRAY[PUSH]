#include <stdio.h>
#define MAX 5
int main() {
    int stack[MAX], top = -1, item, i, n;
    printf("Enter number of elements to push (max %d): ", MAX);
    scanf("%d", &n);
    for(i = 0; i < n; i++) {
        if(top == MAX - 1)
            printf("Stack Overflow! Cannot push more elements.\n");
        else {
            printf("Enter element to push: ");
            scanf("%d", &item);
            top++;
            stack[top] = item;
            printf("Element %d pushed into stack.\n", item);
        }
    }
    printf("\nStack Elements (Top to Bottom): ");
    for(i = top; i >= 0; i--)
        printf("%d ", stack[i]);
    return 0;
}
