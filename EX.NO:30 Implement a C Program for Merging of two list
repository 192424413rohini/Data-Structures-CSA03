#include <stdio.h>

int main() {
    int list1[50], list2[50], merged[100];
    int n1, n2, i, k = 0;

    printf("Enter size of first list: ");
    scanf("%d", &n1);

    printf("Enter elements of first list: ");
    for(i = 0; i < n1; i++)
        scanf("%d", &list1[i]);

    printf("Enter size of second list: ");
    scanf("%d", &n2);

    printf("Enter elements of second list: ");
    for(i = 0; i < n2; i++)
        scanf("%d", &list2[i]);

    // Merging two lists
    for(i = 0; i < n1; i++)
        merged[k++] = list1[i];

    for(i = 0; i < n2; i++)
        merged[k++] = list2[i];

    printf("\nMerged List: ");
    for(i = 0; i < k; i++)
        printf("%d ", merged[i]);

    return 0;
}
