#include <stdio.h>
int main() {
    int a[50], n, i, j, count;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);
    printf("\nElements repeated twice:\n");
    for(i = 0; i < n; i++) {
        count = 1;
        for(j = i + 1; j < n; j++) {
            if(a[i] == a[j]) {
                count++;
            }
        }
        if(count == 2) {
            printf("%d\n", a[i]);
        }
    }
    return 0;
}
