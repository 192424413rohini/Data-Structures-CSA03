#include <stdio.h>
int main() {
    int a[50], n, i;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);
    printf("\nEven numbers: ");
    for(i = 0; i < n; i++) {
        if(a[i] % 2 == 0)
            printf("%d ", a[i]);
    }
    printf("\nOdd numbers: ");
    for(i = 0; i < n; i++) {
        if(a[i] % 2 != 0)
            printf("%d ", a[i]);
    }
    return 0;
}
