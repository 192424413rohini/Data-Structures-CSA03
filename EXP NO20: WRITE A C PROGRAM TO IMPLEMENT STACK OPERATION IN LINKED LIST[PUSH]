#include <stdio.h>
#include <stdlib.h>
struct node {
    int data;
    struct node *next;
};
int main() {
    struct node *top = NULL, *newnode;
    int n, i, item;
    printf("Enter number of elements to push: ");
    scanf("%d", &n);
    for(i = 0; i < n; i++) {
        newnode = (struct node*)malloc(sizeof(struct node));
        printf("Enter element to push: ");
        scanf("%d", &item);
        newnode->data = item;
        newnode->next = top; // Link to previous top
        top = newnode;       // Move top to new node
    }

    printf("\nStack elements (Top to Bottom): ");
    struct node *temp = top;
    while(temp != NULL) {
        printf("%d ", temp->data);
        temp = temp->next;
    }
    return 0;
}
