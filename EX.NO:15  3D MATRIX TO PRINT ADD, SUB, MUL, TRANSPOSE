#include <stdio.h>
int main() {
    int A[3][3], B[3][3], C[3][3], i, j, k, sum;
    printf("Enter 1st 3x3 matrix:\n");
    for(i=0;i<3;i++)
        for(j=0;j<3;j++)
            scanf("%d",&A[i][j]);
    printf("Enter 2nd 3x3 matrix:\n");
    for(i=0;i<3;i++)
        for(j=0;j<3;j++)
            scanf("%d",&B[i][j]);
    printf("\nAddition:\n");
    for(i=0;i<3;i++){for(j=0;j<3;j++){C[i][j]=A[i][j]+B[i][j];printf("%d ",C[i][j]);}printf("\n");}
    printf("\nSubtraction:\n");
    for(i=0;i<3;i++){for(j=0;j<3;j++){C[i][j]=A[i][j]-B[i][j];printf("%d ",C[i][j]);}printf("\n");}
    printf("\nMultiplication:\n");
    for(i=0;i<3;i++){for(j=0;j<3;j++){sum=0;for(k=0;k<3;k++)sum+=A[i][k]*B[k][j];printf("%d ",sum);}printf("\n");}
    printf("\nTranspose of A:\n");
    for(i=0;i<3;i++){for(j=0;j<3;j++)printf("%d ",A[j][i]);printf("\n");}
    return 0;
}
