#include <stdio.h>
int main() {
    int a[50], n, key, i, low, high, mid, found = 0;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Enter %d elements (sorted for binary search):\n", n);
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);
    printf("Enter element to search: ");
    scanf("%d", &key);
    for(i = 0; i < n; i++) {
        if(a[i] == key) {
            printf("Found by Linear Search at position %d\n", i + 1);
            found = 1;
            break;
        }
    }
    if(!found)
        printf("Not found by Linear Search\n");
        low = 0; 
    high = n - 1; 
    found = 0;
    while(low <= high) {
        mid = (low + high) / 2;
        if(a[mid] == key) {
            printf("Found by Binary Search at position %d\n", mid + 1);
            found = 1;
            break;
        } else if(key < a[mid])
            high = mid - 1;
        else
            low = mid + 1;
    }
    if(!found)
        printf("Not found by Binary Search\n");
    return 0;
}
