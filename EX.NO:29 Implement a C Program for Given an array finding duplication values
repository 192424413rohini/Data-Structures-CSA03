#include <stdio.h>

int main() {
    int arr[50], n, i, j;

    printf("Enter size of array: ");
    scanf("%d", &n);

    printf("Enter array elements: ");
    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("\nDuplicate values: ");

    for(i = 0; i < n; i++) {
        for(j = i + 1; j < n; j++) {
            if(arr[i] == arr[j]) {
                printf("%d ", arr[i]);
                break;  // avoid printing same duplicate multiple times
            }
        }
    }

    return 0;
}
