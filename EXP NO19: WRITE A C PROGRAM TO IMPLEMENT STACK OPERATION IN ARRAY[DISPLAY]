#include <stdio.h>
#define MAX 5
int main() {
    int stack[MAX], top = -1, item, n, i;
    printf("Enter number of elements to push (max %d): ", MAX);
    scanf("%d", &n);
    for(i = 0; i < n; i++) {
        if(top == MAX - 1)
            printf("Stack Overflow!\n");
        else {
            printf("Enter element: ");
            scanf("%d", &item);
            stack[++top] = item;
        }
    }
    printf("\n--- Stack Display ---\n");
    if(top == -1)
        printf("Stack is empty\n");
    else {
        printf("Stack elements (Top to Bottom): ");
        for(i = top; i >= 0; i--)
            printf("%d ", stack[i]);
        printf("\n");
    }
    return 0;
}
